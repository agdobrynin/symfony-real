<nav aria-label="Page navigation"><ul class="pagination justify-content-center">
    {% for i in 1..pageCount %}
        {% if app.request.attributes.get("page") == i %}
            <li class="page-item disabled"><a class="page-link" href="#" tabindex="-1">{{ i }}</a></li>
        {% else %}
            <li class="page-item"><a class="page-link" href="{{ path("blog_list", {page: i}) }}">{{ i }}</a></li>
        {% endif %}
    {% endfor %}
</ul></nav>
